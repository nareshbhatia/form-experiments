{
  "name": "form-experiments",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "clean": "rimraf node_modules dist",
    "dev": "vite",
    "fix": "run-s _lint:fix _format:fix",
    "gen": "ts-to-zod src/types/types.ts src/types/schema.ts",
    "lint": "run-s _lint _format",
    "preview": "vite preview",
    "_format": "prettier --list-different '*.{js,ts,json,md}' 'src/**/*.{css,js,jsx,ts,tsx,json,md}'",
    "_format:fix": "npm run _format -- --write",
    "_lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "_lint:fix": "npm run _lint -- --fix"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/vite": "^4.0.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.484.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-router": "^7.4.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.16",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@code-shaper/eslint-config": "^2.*",
    "@types/node": "^22.13.13",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.57.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-to-zod": "^3.15.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.3"
  }
}
